{
  "expo": {
    "name": "Reci",
    "slug": "Reci",
    "version": "1.0.0",
    "description": "An application to store recipes.",
    "orientation": "portrait",
    "icon": "./app/assets/reci-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lailaadranly.Reci"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "78e4ff06-0638-48cb-8e64-53e0ae7d1656"
      }
    }
  }
}
